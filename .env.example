# Copy this file to .env and fill in your values
GITHUB_USERNAME=AswinBarath
GITHUB_TOKEN=
# Update cadence (cron-like in ms). Default: 2 hours
UPDATE_INTERVAL_MS=7200000
# Keep a buffer of remaining requests before pausing updates (0..1)
